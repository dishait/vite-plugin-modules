{
  "version": "1.3.1",
  "name": "vite-plugin-use-modules",
  "description": "自动加载 modules",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dishait/vite-plugin-use-modules.git"
  },
  "exports": {
    "./*": "./*",
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub && pnpm -C examples/vite dev",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "keywords": [
    "vite",
    "vue",
    "module",
    "plugin"
  ],
  "author": {
    "name": "markthree",
    "url": "https://github.com/markthree"
  },
  "bugs": {
    "url": "https://github.com/dishait/vite-plugin-use-modules/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.16.13",
    "bumpp": "^9.1.0",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.8",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "vue": ">=3.0.0",
    "vite": ">=2.0.0"
  },
  "dependencies": {
    "local-pkg": "^0.4.3"
  }
}
